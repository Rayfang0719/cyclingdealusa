.custom-wrapper {
  background-color: #EAEAEA;
  color: #000;
  font-weight: 700;
  text-transform: uppercase;
  -webkit-font-smoothing: antialiased;

  overflow: hidden;
  -webkit-transition: height 0.5s;
  -moz-transition: height 0.5s;
  -ms-transition: height 0.5s;
  transition: height 0.5s;
  padding-top: 12px;
  padding-bottom: 12px;
  margin-bottom: -2px;
}

.custom-wrapper.open {
